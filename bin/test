#!/bin/sh


dash="$(dirname $(find ~/.emacs.d | grep "/dash.el$" | tail -1))"
s_el="$(dirname $(find ~/.emacs.d | grep "/s.el$" | tail -1))"

echo $dash
echo $s_el

emacs --batch \
  -eval "(add-to-list 'load-path \"${dash}\")" \
  -eval "(add-to-list 'load-path \"${s_el}\")" \
  -eval "(add-to-list 'load-path \".\")" \
  -l ./etd.el \
  -l dash \
  -l ./kurecolor.el \
  -l ert \
  -l ./kurecolor-examples.el \
  --eval "(ert-run-tests-batch-and-exit)"
